        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "TransactionType": {
                        "type": "string",
                        "description": "Transaction Type. Possible values: CashIn, CashOut, ReceivedTransfers, OrderedTransfers."
                      },
                      "Amounts": {
                        "type": "array",
                        "items": {
                          "$ref": "#/components/schemas/Codification"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "Example": {
                    "value": [
                      {
                        "TransactionType": "string",
                        "Amounts": [
                          {
                            "Value": 0,
                            "Name": "string"
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "schema": {
            "type": "string",
            "enum": [
              "Individuals",
              "LegalEntities"
            ]
          },
          "name": "CustomerType",
          "in": "path",
          "required": true,
          "description": "Customer Type. Allowed values: Individuals, LegalEntities."
